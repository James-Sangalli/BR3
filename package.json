{
  "name": "BitReturn",
  "version": "3.0.0",
  "main": "server.js",
  "scripts": {
    "knex-build": "knex migrate:rollback && knex migrate:latest && knex seed:run",
    "server": "nodemon server.js",
    "search": "nodemon searchServer.js",
    "bundle": "browserify js/search.js > js/searchBundle.js",
    "pay": "nodemon pay.js",
