  },
  "homepage": "https://github.com/James-Sangalli/BitReturn#readme",
  "dependencies": {
    "api-client-limiter": "^0.1.0",
    "bitcoinjs-lib": "^2.2.0",
    "bitcore-lib": "^0.14.0",
    "body-parser": "^1.15.1",
    "browserify": "^13.0.0",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "express-limiter": "^1.6.0",
    "knex": "^0.10.0",
    "mandrill": "^0.1.0",
    "node-mandrill": "^1.0.1",
    "prompt": "^1.0.0",
